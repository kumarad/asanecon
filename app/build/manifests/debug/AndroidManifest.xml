<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:permission="android.permission.RECEIVE_BOOT_COMPLETED" android:versionCode="1" android:versionName="1.0" package="com.techan">
    <uses-sdk android:minSdkVersion="17" android:targetSdkVersion="17"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>

    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name">

        <activity android:label="@string/app_name" android:name="com.techan.activities.StockHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.techan.activities.SettingsActivity" android:parentActivityName="com.techan.activities.StockHomeActivity" android:windowSoftInputMode="stateVisible|adjustResize">

            <intent-filter>
                <action android:name="android.intent.action.MANAGE_NETWORK_USAGE"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>

        </activity>

        <activity android:name="com.techan.activities.StockDetailFragmentActivity" android:parentActivityName="com.techan.activities.StockHomeActivity" android:windowSoftInputMode="stateVisible|adjustResize">
        </activity>

        <provider android:authorities="com.techan.contentprovider" android:name="com.techan.contentProvider.StockContentProvider">
        </provider>

        <receiver android:name="com.techan.alarm.AlarmReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>

        <service android:name="com.techan.alarm.DownloaderService">
        </service>
    </application>
</manifest>
